{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard-with-typescript",
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-trailing-spaces": "warn",
    "padded-blocks": "warn",
    "eol-last": "warn",
    "object-curly-spacing": "warn",
    "@typescript-eslint/explicit-function-return-type": "off"
  }
}
